<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andriod.course">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ServiceActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service
            android:name=".MyService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".SplashActivity">

        </activity>
        <activity android:name=".SQliteActivity" />
        <activity android:name=".SharedPreferencesAndNotificationActivity" />
        <activity android:name=".ProductActivity" />
        <activity android:name=".StudentActivity" />
        <activity android:name=".SimpleCalculatorActivity" />
        <activity
            android:name=".FullSignupActivity"
            android:configChanges="keyboard">
            <intent-filter>
                <action android:name="asdf" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>